{
  "require": {
    "illuminate/database": "5.3",
    "jenssegers/blade": "^1.1",
    "zohocrm/php-sdk": "1.1.8"
  },
  "autoload": {
    "psr-4": {
      "App\\": "wp-content/themes/gotravel-child/App"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "symfony/dom-crawler": "^4.2",
    "symfony/css-selector": "^4.2"
  },
  "scripts": {
    "test": "phpunit",
    "test-all": "phpunit --configuration tests/test-config.xml --coverage-html --testdox",
    "test-contract": "phpunit --configuration tests/contract-test-config.xml --coverage-html --testdox",
    "test-checkout": "@test"
  }
}
